{
    "manifest_version": 3,
    "name": "Form Copilot",
    "version": "1.0.0",
    "description": "A Chrome extension that helps you fill out forms faster",
    "action": {
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}